model country_league_visibility {
  id            Int       @id @default(autoincrement())
  country       String    @db.VarChar(128)
  league        String?   @db.VarChar(128)
  visible       Boolean   @default(true)
  priority      Int       @default(0)
  reason        String?   @db.Text
  effectiveFrom DateTime? @db.Timestamptz(0)
  effectiveTo   DateTime? @db.Timestamptz(0)

  optionId Int?
  option   stat_size_finalize_master? @relation(fields: [optionId], references: [id], onDelete: Cascade)

  register_id   String?   @db.VarChar(100)
  register_time DateTime? @db.Timestamptz(0)
  update_id     String?   @db.VarChar(100)
  update_time   DateTime? @db.Timestamptz(0)

  @@unique([country, league, optionId], map: "uk_web_visibility_scoped")
  @@index([country, league], map: "idx_web_visibility_lookup")
  @@index([visible], map: "idx_web_visibility_flag")
  @@index([optionId], map: "idx_web_visibility_option")
}

model user_visibility_option {
  id        Int      @id @default(autoincrement())
  userId    String   @db.VarChar(64) // ユーザーDBのIDを文字列で格納（FKは張らない）
  optionId  Int
  createdAt DateTime @default(now())

  // stat_size_finalize_master への多対一
  option stat_size_finalize_master @relation(fields: [optionId], references: [id], onDelete: Cascade)

  @@unique([userId, optionId], map: "uk_user_option")
  @@index([userId], map: "idx_user_option_user")
  @@index([optionId], map: "idx_user_option_option")
}

